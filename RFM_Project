   "source": [
    "#Group by CustomerID to calculate RFM\n",
    "rfm= df.groupby('CustomerID').agg({\n",
    "    'InvoiceDate':lambda x:(snapshot_date-x.max()).days, #Recency\n",
    "    'InvoiceNo' : 'nunique',#Frequency\n",
    "    'TotalPrice':'sum'#Monetary\n",
    "})\n",
    "rfm.columns = ['Recency','Frequency','Monetary']\n",
    "rfm.reset_index()\n",
    "rfm.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "9b3ce065-915d-44b8-94b3-8fc086624706",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Assign RFM Scores (1–5)\n",
    "# Recency: lower is better → reverse scoring\n",
    "rfm['r_score']=pd.qcut(rfm['Recency'],5,labels=[5,4,3,2,1])\n",
    "#frequency: more is better \n",
    "rfm['f_score']=pd.qcut(rfm['Frequency'].rank(method='first'),5,labels=[1,2,3,4,5])\n",
    "#Monetary: more is better\n",
    "rfm['m_score']=pd.qcut(rfm['Monetary'],5,labels=[1,2,3,4,5])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "f8f2914d-40cd-45e0-83e6-8b5dc0e395d5",
